.list .item input{width: 500rpx; line-height: 82rpx;height: 82rpx;}
.btm{margin: 40rpx 30rpx 0 30rpx;border: 0;border-radius: 10rpx;height: 100rpx;line-height: 100rpx;font-size: 38rpx;color: #fff;background-color: #09bb07;}