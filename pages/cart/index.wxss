page{background-color: #f0eff4}
.parent{width: 100%;overflow: hidden;position: relative;height: 240rpx;border-bottom: 2rpx solid #d0d0d0;background-color: #fff;}
.item{padding: 20rpx 0;position: absolute;top: 0;height: 190rpx;box-sizing: content-box;width: 1000rpx;}
.item label{height: 190rpx;width: 88rpx;position: relative;float: left; line-height:190rpx; text-align: center } 
.item image{height: 190rpx;width: 142rpx;float: left;margin-right: 20rpx;}
.item .content{display: inline-flex;flex-direction: column;justify-content: space-around;width: 480rpx;height: 190rpx;margin-right: 40rpx;color: #a3a3a3;font-size: 32rpx;float: left;}
.item .content .big{display: inline-block;width: 100%;overflow: hidden;height: 36rpx;line-height: 36rpx;}
.item .delete{height: 190rpx;width: 160rpx;text-align: center;display: inline-block;line-height: 190rpx;background-color: #e94f4f;color: #fff;font-size: 36rpx;}
.item .content .bookname{font-size: 36rpx;color: #333;}
.item .content .money{color: #e94f4f;font-size: 28rpx;}
.item .content .money .len{float: right; color: #333;}
.dispatching{padding: 20rpx 0 30rpx 30rpx;margin-top: 20rpx;background-color: #fff;}
.dispatching .h1{border-bottom: 2rpx solid #eeeeee;font-size: 36rpx;line-height: 78rpx;color: #999;}
.go_buy{background-color: #fff;position: fixed;z-index: 9;width: 100%;height: 90rpx;line-height: 90rpx;left: 0;bottom: 0;}
.go_buy label{height: 90rpx;width: 60rpx;position: relative;float: left;margin-left: 20rpx; line-height: 90rpx}
.go_buy label checkbox{  z-index: 3;}
.go_buy .left{float: left;color: #333;font-size: 36rpx;}
.go{float: right;width: 360rpx;}
.go .all{color: #929292;font-size: 28rpx;float: left;}
.go .all view{color: #ef4d42;display: inline-block;}
.go .next{background-color: #ff5500;text-align: center;color: #fff;width: 190rpx;float: right;font-size: 32rpx;}
